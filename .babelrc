{
  "plugins": [
    [
      "module:react-native-dotenv",
      {
        "envName": "local",
        "moduleName": "@env",
        "path": ".env.local",
        "allowUndefined": false,
        "safe": true,
        "allowlist": [
          "HTTP_PROTOCOL",
          "WS_PROTOCOL",
          "API_ORIGIN"
        ]
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "@common": "./src/common/index",
          "@app-ctx": "./src/AppContext"
        }
      }
    ]
  ]
}
